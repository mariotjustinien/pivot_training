{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "training"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/metro-ui/build/css/metro.min.css",
        "../node_modules/metro-ui/build/css/metro.css",
        "../node_modules/metro-ui/build/css/metro-icons.css",
        "../node_modules/metro-ui/build/css/metro-icons.min.css",
        "../node_modules/metro-ui/build/css/metro-responsive.css",
        "../node_modules/metro-ui/build/css/metro-responsive.min.css",
        "../node_modules/metro-ui/build/css/metro-rtl.css",
        "../node_modules/metro-ui/build/css/metro-rtl.min.css",
        "../node_modules/metro-ui/build/css/metro-schemes.css",
        "../node_modules/metro-ui/build/css/metro-schemes.min.css",
        "../node_modules/metro-ui/docs/css/metro.css",
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/font-awesome/css/font-awesome.min.css"
      ],
      "scripts": [
        "../node_modules/metro-ui/build/js/metro.min.js",
       "../node_modules/metro-ui/build/js/metro.js",
       "../node_modules/metro-ui/docs/js/select2.min.js",
       "../node_modules/metro-ui/docs/js/docs.js",
       "../node_modules/metro-ui/docs/js/metro.js",
       "../node_modules/metro-ui/docs/js/jquery-2.1.3.min.js",
       "../node_modules/metro-ui/docs/js/jquery.dataTables.min.js",
       "../node_modules/metro-ui/docs/js/ga.js",
       "../node_modules/metro-ui/js/widgets/dropdown.js",
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
